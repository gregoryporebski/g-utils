{
  "name": "@gregoryporebski/g-utils",
  "version": "1.0.0",
  "description": "Generic, type safe and lightweight utilities for TypeScript",
  "license": "GNU GPLv3",
  "author": {
    "name": "Grzegorz PorÄ™bski",
    "email": "gregory.porebski@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoryporebski/g-utils.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "keywords": [
    "typescript",
    "utils",
    "generic",
    "type-safe",
    "lightweight",
    "functional"
  ],
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "bun clean && bun build.ts && tsc -p tsconfig.dts.json",
    "clean": "rm -rf lib doc",
    "doc": "typedoc src --out doc --plugin @mxssfd/typedoc-theme --theme my-theme",
    "doc:markdown": "bun doc --plugin typedoc-plugin-markdown",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "latest",
    "@semantic-release/commit-analyzer": "latest",
    "@semantic-release/github": "latest",
    "@semantic-release/npm": "latest",
    "@semantic-release/release-notes-generator": "latest",
    "bun-types": "latest",
    "prettier": "latest",
    "semantic-release": "latest",
    "typedoc": "latest",
    "typedoc-plugin-markdown": "latest"
  },
  "peerDependencies": {
    "typesafe-utils": "latest"
  }
}
